#!/home/rcir178/boiler-plate/bin/python3.6
# EASY-INSTALL-ENTRY-SCRIPT: 'Boiler-Plate','console_scripts','boiler-plate'
__requires__ = 'Boiler-Plate'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('Boiler-Plate', 'console_scripts', 'boiler-plate')()
    )
